    "metro-react-native-babel-transformer": "0.72.3",
    "metro-resolver": "0.72.3",
    "nx": "15.0.10",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "react-native-config": "1.4.6",
    "react-native-svg": "13.2.0",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
