{
  "name": "nx-react-native",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "android": "nx run-android",
    "build": "nx build",
    "ios": "nx run-ios",
    "lint": "nx affected:lint",
    "pod-install": "nx affected --target=pod-install --parallel=1",
    "start": "nx serve",
    "test": "nx affected:test",
    "test-coverage": "rm -rf coverage && npx nx run-many --all --target=test --configuration=coverage --skip-nx-cache && node tools/combine-coverage-final.js && nyc merge coverage coverage/combine-coverage/coverage.json && nyc report -t coverage/combine-coverage --report-dir coverage/combine-coverage --reporter=html --reporter=lcov",
    "type-check": "nx affected --target=type-check",
    "prepare": "husky install"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "15.0.10",
    "@nrwl/detox": "15.0.10",
    "@nrwl/eslint-plugin-nx": "15.0.10",
    "@nrwl/jest": "15.0.10",
    "@nrwl/linter": "15.0.10",
    "@nrwl/react-native": "15.0.10",
    "@nrwl/web": "15.0.10",
    "@nrwl/workspace": "15.0.10",
    "@react-native-async-storage/async-storage": "1.17.10",
    "@react-native-community/cli": "9.1.1",
    "@react-native-community/cli-platform-android": "9.1.0",
    "@react-native-community/cli-platform-ios": "9.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/jest-native": "4.0.13",
    "@testing-library/react-native": "11.2.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.20",
    "@types/react-native": "0.70.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "detox": "19.12.5",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "jest": "28.1.1",
    "jest-circus": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-react-native": "18.0.0",
    "metro": "0.72.3",
    "metro-babel-register": "0.72.3",
